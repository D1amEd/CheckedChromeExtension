{
  "manifest_version": 3,
  "name": "Checked",
  "description": "Extensión de chrome destinada a la detección de hechos falsos",
  "version": "1.0",
  "icons": {
    "128": "images/Checked.png"
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  
  "action": {
    "default_popup": "popup.html"
  }
}